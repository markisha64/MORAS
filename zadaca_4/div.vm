push constant 5
push constant 2
call div.Div 2
return

function div.Div 2
	// Q = 0
	push constant 0
	pop local 0 

	// R = N
	push argument 0
	pop local 1

label divLoop

	// while R >= D
	push local 1
	push argument 1
	lt
	if-goto divDone

	// Q = Q + 1
	push local 0
	push constant 1
	add
	pop local 0

	// R = R - D
	push local 1
	push argument 1
	sub
	pop local 1

	goto divLoop
	
label divDone
	// return (Q, R)
	push local 0
	pop temp 0
	push local 1
	pop temp 1
	return
